	langid = {english},
	keywords = {Behavioral Context, Call Type, Diel Pattern, Humpback Whale, Sound Production},
	file = {Full Text PDF:C\:\\Users\\Shannon\\Zotero\\storage\\JEAA4SHM\\Širović et al. - 2013 - Temporal separation of two fin whale call types ac.pdf:application/pdf},
}

